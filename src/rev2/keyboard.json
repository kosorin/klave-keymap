{
    // Hardware
    "usb": {
        "device_version": "2.0.0"
    },
    "board": "GENERIC_RP_RP2040",
    "processor": "RP2040",
    "bootloader": "rp2040",

    // Split
    "split": {
        "handedness": {
            "pin": "GP22"
        },
        "serial": {
            "driver": "vendor"
        },
        "transport": {
            "protocol": "serial"
        }
    },

    // Matrix
    "debounce": 5,
    "diode_direction": "COL2ROW",
    "matrix_pins": {
        "cols": [ "GP10", "GP11", "GP12", "GP13", "GP14", "GP15" ],
        "rows": [ "GP20", "GP19", "GP18", "GP17", "GP16" ]
    },

    // Layouts
    "layouts": {
        "LAYOUT": {
            "layout": [
                // left row 0
                { "label": "L00", "matrix": [ 0, 0 ], "x": 0, "y": 1 },
                { "label": "L01", "matrix": [ 0, 1 ], "x": 1, "y": 1 },
                { "label": "L02", "matrix": [ 0, 2 ], "x": 2, "y": 0.25 },
                { "label": "L03", "matrix": [ 0, 3 ], "x": 3, "y": 0 },
                { "label": "L04", "matrix": [ 0, 4 ], "x": 4, "y": 0.4 },
                { "label": "L05", "matrix": [ 0, 5 ], "x": 5, "y": 0.55 },
                // right row 0
                { "label": "R05", "matrix": [ 5, 5 ], "x": 10, "y": 0.55 },
                { "label": "R04", "matrix": [ 5, 4 ], "x": 11, "y": 0.4 },
                { "label": "R03", "matrix": [ 5, 3 ], "x": 12, "y": 0 },
                { "label": "R02", "matrix": [ 5, 2 ], "x": 13, "y": 0.25 },
                { "label": "R01", "matrix": [ 5, 1 ], "x": 14, "y": 1 },
                { "label": "R00", "matrix": [ 5, 0 ], "x": 15, "y": 1 },

                // left row 1
                { "label": "L10", "matrix": [ 1, 0 ], "x": 0, "y": 2 },
                { "label": "L11", "matrix": [ 1, 1 ], "x": 1, "y": 2 },
                { "label": "L12", "matrix": [ 1, 2 ], "x": 2, "y": 1.25 },
                { "label": "L13", "matrix": [ 1, 3 ], "x": 3, "y": 1 },
                { "label": "L14", "matrix": [ 1, 4 ], "x": 4, "y": 1.4 },
                { "label": "L15", "matrix": [ 1, 5 ], "x": 5, "y": 1.55 },
                // right row 1
                { "label": "R15", "matrix": [ 6, 5 ], "x": 10, "y": 1.55 },
                { "label": "R14", "matrix": [ 6, 4 ], "x": 11, "y": 1.4 },
                { "label": "R13", "matrix": [ 6, 3 ], "x": 12, "y": 1 },
                { "label": "R12", "matrix": [ 6, 2 ], "x": 13, "y": 1.25 },
                { "label": "R11", "matrix": [ 6, 1 ], "x": 14, "y": 2 },
                { "label": "R10", "matrix": [ 6, 0 ], "x": 15, "y": 2 },

                // left row 2
                { "label": "L20", "matrix": [ 2, 0 ], "x": 0, "y": 3 },
                { "label": "L21", "matrix": [ 2, 1 ], "x": 1, "y": 3 },
                { "label": "L22", "matrix": [ 2, 2 ], "x": 2, "y": 2.25 },
                { "label": "L23", "matrix": [ 2, 3 ], "x": 3, "y": 2 },
                { "label": "L24", "matrix": [ 2, 4 ], "x": 4, "y": 2.4 },
                { "label": "L25", "matrix": [ 2, 5 ], "x": 5, "y": 2.55 },
                // right row 2
                { "label": "R25", "matrix": [ 7, 5 ], "x": 10, "y": 2.55 },
                { "label": "R24", "matrix": [ 7, 4 ], "x": 11, "y": 2.4 },
                { "label": "R23", "matrix": [ 7, 3 ], "x": 12, "y": 2 },
                { "label": "R22", "matrix": [ 7, 2 ], "x": 13, "y": 2.25 },
                { "label": "R21", "matrix": [ 7, 1 ], "x": 14, "y": 3 },
                { "label": "R20", "matrix": [ 7, 0 ], "x": 15, "y": 3 },

                // left thumb row 0
                { "label": "L34", "matrix": [ 3, 4 ], "x": 6, "y": 3 },
                { "label": "L35", "matrix": [ 3, 5 ], "x": 7, "y": 3.25 },
                // right thumb row 0
                { "label": "R35", "matrix": [ 8, 5 ], "x": 8, "y": 3.25 },
                { "label": "R34", "matrix": [ 8, 4 ], "x": 9, "y": 3 },

                // left thumb row 1
                { "label": "L41", "matrix": [ 4, 1 ], "x": 2.5, "y": 3.55 },
                { "label": "L42", "matrix": [ 4, 2 ], "x": 3.5, "y": 3.4 },
                { "label": "L43", "matrix": [ 4, 3 ], "x": 4.5, "y": 3.55 },
                { "label": "L44", "matrix": [ 4, 4 ], "x": 5.5, "y": 4 },
                { "label": "L45", "matrix": [ 4, 5 ], "x": 6.5, "y": 4.25 },
                // right thumb row 1
                { "label": "R45", "matrix": [ 9, 5 ], "x": 8.5, "y": 4.25 },
                { "label": "R44", "matrix": [ 9, 4 ], "x": 9.5, "y": 4 },
                { "label": "R43", "matrix": [ 9, 3 ], "x": 10.5, "y": 3.55 },
                { "label": "R42", "matrix": [ 9, 2 ], "x": 11.5, "y": 3.4 },
                { "label": "R41", "matrix": [ 9, 1 ], "x": 12.5, "y": 3.55 }
            ]
        }
    }
}
