{
    "keyboard_name": "Klave",
    "url": "https://github.com/kosorin/klave",
    "maintainer": "David Kosorin",
    "manufacturer": "David Kosorin",
    "usb": {
        "vid": "0x444B",
        "pid": "0x0001",
        "device_version": "1.0.0"
    },
    "development_board": "promicro",
    "processor": "atmega32u4",
    "bootloader": "caterina",
    "build": {
        "lto": true
    },
    "split": {
        "enabled": true,
        "soft_serial_pin": "D0"
    },
    "debounce": 5,
    "diode_direction": "COL2ROW",
    "matrix_pins": {
        "cols": [ "B5", "B4", "E6", "D7", "C6", "D4" ],
        "rows": [ "B6", "B2", "B3", "B1", "F7", "F6" ]
    }
}

/*

Pins:
- D0 (SCL)
- D1 (SDA)
- D2
- D3

LEDs:
- B0 Left
- D5 Right

writePinLow(<led>) - turn on
writePinHigh(<led>) - turn off

*/
